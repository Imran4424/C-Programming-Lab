#include <stdio.h>

int main()
{
	char grade[5];
	int x;
	scanf("%d",&x);
	if(x>=80){
		printf("the grade is A+\n",grade[5]);
	}	
	else if(x>=70){
		printf("the grade is A\n",grade[5]);
	}	
	else if(x>=50){
		printf("the grade is B\n",grade[5]);
	}		
	else if(x>=60){
		printf("the grade is A-\n",grade[5]);
	}	
	else if(x>=50){
		printf("the grade is B\n",grade[5]);
	}	
	else if(x>=40){
		printf("the grade is C\n",grade[5]);
	}
	else if(x>=33){
		printf("the grade is E\n",grade[5]);
	}	
	else{
		printf("the grade is F\n",grade[5]);
	}	
		
	return 0;
}							
