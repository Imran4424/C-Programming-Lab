#include <stdio.h>

int main()
{
	char ch[3];
	scanf("%s",ch[3]);
	switch(ch[3]){
		case 'CC':
			printf("i love c\n");
			break;
		case 'HH':
			printf("i hate c\n");
			break;
			case 'DH':
			printf("i die for c\n");
			break;
			case 'MH':
			printf("i am mad for c\n");
			break;
			
		default:
			printf("Hihi...haha\n");
			break;
	}
	return 0;

}				
